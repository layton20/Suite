  </PropertyGroup>

  <ItemGroup>
    <Folder Include="wwwroot\css\vendor\" />
    <Folder Include="wwwroot\js\vendor\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.2" />
